#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/pkgroll@2.12.2_typescript@5.8.3/node_modules/pkgroll/dist/node_modules:/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/pkgroll@2.12.2_typescript@5.8.3/node_modules/pkgroll/node_modules:/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/pkgroll@2.12.2_typescript@5.8.3/node_modules:/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/pkgroll@2.12.2_typescript@5.8.3/node_modules/pkgroll/dist/node_modules:/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/pkgroll@2.12.2_typescript@5.8.3/node_modules/pkgroll/node_modules:/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/pkgroll@2.12.2_typescript@5.8.3/node_modules:/home/cristian/Proyectos/prueba-tecnica/order-service/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.mjs" "$@"
else
  exec node  "$basedir/../../dist/cli.mjs" "$@"
fi
